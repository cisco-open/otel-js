{
  "name": "cisco-telescope",
  "version": "2.0.1",
  "description": "Cisco Distribution for OpenTelemetry",
  "author": "Cisco Epsagon Team <support@epsagon.com>",
  "license": "MIT",
  "homepage": "https://github.com/cisco-open/otel-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/cisco-open/otel-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "lerna run clean",
    "prepare": "husky install",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "lint": "eslint . --ext .ts",
    "lint:commits": "commitlint",
    "build": "tsc -p .",
    "test": "lerna run test",
    "test:ci:changed": "lerna run test --since origin/main",
    "codecov": "lerna run codecov",
    "semantic-release": "lerna run semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "@cisco-telescope/auto-instrumentations-node": "file:packages/auto-instrumentations-node",
    "@cisco-telescope/cisco-sdk-node": "file:packages/cisco-sdk-node"
  },
  "devDependencies": {
    "@aws-sdk/client-sns": "^3.49.0",
    "@aws-sdk/client-sqs": "^3.54.0",
    "@commitlint/cli": "16.0.2",
    "@commitlint/config-conventional": "16.0.0",
    "@opentelemetry/resources": "^1.0.1",
    "@types/amqplib": "^0.5.17",
    "@types/jasmine": "^3.10.3",
    "@types/jest": "^27.4.0",
    "@types/mocha": "9.0.0",
    "@types/node": "^17.0.8",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "amqplib": "^0.7.1",
    "assert": "^2.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.3.6",
    "codecov": "3.7.2",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "expect": "^26.6.2",
    "grpc": "1.24.11",
    "grpc_tools_node_protoc_ts": "^5.0.0",
    "gts": "^3.1.0",
    "husky": "^7.0.4",
    "lerna": "5.1.4",
    "lerna-changelog": "1.0.1",
    "mocha": "^9.2.0",
    "nock": "^13.2.4",
    "prettier": ">=2.3.2",
    "redis": "^3.0.0",
    "semantic-release": "^19.0.2",
    "sinon": "^12.0.1",
    "ts-mocha": "9.0.2",
    "typescript": "4.4.4"
  }
}
