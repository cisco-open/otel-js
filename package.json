{
  "name": "cisco-telescope",
  "version": "0.0.0-development",
  "description": "Cisco Distribution for OpenTelemetry",
  "author": "Cisco Epsagon Team <support@epsagon.com>",
  "license": "MIT",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "homepage": "https://github.com/epsagon/otel-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/epsagon/otel-js.git"
  },
  "scripts": {
    "prepare": "husky install",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "lint": "eslint . --ext .ts",
    "lint:commits": "commitlint",
    "build": "tsc -p .",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p .",
    "semantic-release": "semantic-release",
    "rabbitmq": "docker run -p 5672:5672 --env RABBITMQ_DEFAULT_USER=username --env RABBITMQ_DEFAULT_PASS=password rabbitmq:3",
    "redis": "docker run -p 6379:6379 redis"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "index.js",
    "bin/*.js",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "lib/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/auto-instrumentations-node": "^0.28.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.27.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.27.0",
    "@opentelemetry/instrumentation": "^0.27.0",
    "@opentelemetry/instrumentation-aws-sdk": "^0.5.0",
    "@opentelemetry/instrumentation-grpc": "^0.27.0",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/instrumentation-redis": "^0.28.0",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "~1.0.1",
    "@opentelemetry/sdk-trace-node": "~1.0.1",
    "opentelemetry-instrumentation-amqplib": "^0.27.0",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "cisco-opentelemetry-specifications": "0.0.15",
    "express": "^4.17.2",
    "nyc": "^15.1.0"
  },
  "devDependencies": {
    "@aws-sdk/client-sns": "^3.49.0",
    "@aws-sdk/client-sqs": "^3.54.0",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@opentelemetry/resources": "^1.0.1",
    "@grpc/grpc-js": "1.4.4",
    "@types/amqplib": "^0.5.17",
    "@types/jasmine": "^3.10.3",
    "@types/jest": "^27.4.0",
    "@types/mocha": "9.0.0",
    "@types/node": "^17.0.8",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "amqplib": "^0.7.1",
    "assert": "^2.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.3.6",
    "codecov": "3.7.2",
    "eslint": "^8.1.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expect": "^26.6.2",
    "grpc": "1.24.11",
    "grpc_tools_node_protoc_ts": "^5.0.0",
    "gts": "3.1.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.0",
    "nock": "^13.2.4",
    "prettier": ">=2.3.2",
    "redis": "^3.0.0",
    "semantic-release": "^19.0.2",
    "sinon": "^12.0.1",
    "ts-mocha": "9.0.2",
    "typescript": "4.4.4"
  }
}
