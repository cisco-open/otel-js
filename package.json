{
  "name": "cisco-opentelemetry-node",
  "version": "0.0.0-development",
  "description": "Epsagon Instrumentation for Node.js",
  "author": "Cisco Epsagon Team <support@epsagon.com>",
  "license": "MIT",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "homepage": "https://github.com/epsagon/otel-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/epsagon/otel-js.git"
  },
  "scripts": {
    "prepare": "husky install",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "lint": "eslint . --ext .ts",
    "lint:commits": "commitlint",
    "build": "tsc -p .",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p .",
    "semantic-release": "semantic-release"
  },
  "files": [
    "index.js",
    "bin/*.js",
    "lib/src/*.js",
    "lib/src/*.js.map",
    "lib/src/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/auto-instrumentations-node": "^0.27.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.27.0",
    "@opentelemetry/instrumentation": "^0.27.0",
    "@opentelemetry/instrumentation-http": "^0.27.0",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "~1.0.1",
    "@opentelemetry/sdk-trace-node": "~1.0.1",
    "express": "^4.17.2",
    "nyc": "^15.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "@types/jasmine": "^3.10.3",
    "@types/jest": "^27.4.0",
    "@types/mocha": "9.0.0",
    "@types/node": "^17.0.8",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "codecov": "3.7.2",
    "eslint": "^8.1.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gts": "3.1.0",
    "husky": "^7.0.4",
    "mocha": "^8.4.0",
    "prettier": ">=2.3.2",
    "sinon": "^12.0.1",
    "ts-mocha": "8.0.0",
    "typescript": "4.4.4",
    "semantic-release": "^19.0.2"
  }
}
