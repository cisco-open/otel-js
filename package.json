{
  "name": "cisco-telescope",
  "version": "0.0.0-development",
  "description": "Cisco Distribution for OpenTelemetry",
  "author": "Cisco Epsagon Team <support@epsagon.com>",
  "license": "MIT",
  "homepage": "https://github.com/epsagon/otel-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/epsagon/otel-js.git"
  },
  "scripts": {
    "clean": "lerna run clean",
    "prepare": "husky install",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "lint": "eslint . --ext .ts",
    "lint:commits": "commitlint",
    "build": "tsc -p .",
    "test": "lerna run test",
    "test:ci:changed": "lerna run test --since origin/main",
    "codecov": "lerna run codecov",
    "semantic-release": "lerna run semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "nyc": "^15.1.0",
    "pkginfo": "^0.4.1"
  },
  "devDependencies": {
    "codecov": "3.7.2",
    "gts": "3.1.0",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.4",
    "lerna": "5.1.4",
    "lerna-changelog": "1.0.1",
    "husky": "^7.0.4",
    "semantic-release": "^19.0.2",
    "prettier": ">=2.3.2",
    "typescript": "4.4.4",
    "@commitlint/cli": "16.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "@commitlint/config-conventional": "16.0.0"
  }
}
