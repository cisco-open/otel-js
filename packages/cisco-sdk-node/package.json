{
  "name": "@cisco-telescope/cisco-sdk-node",
  "version": "2.0.1",
  "description": "Cisco Distribution for OpenTelemetry",
  "author": "Cisco Epsagon Team <support@epsagon.com>",
  "license": "MIT",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "homepage": "https://github.com/cisco-open/otel-js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/cisco-open/otel-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "tsc --build --clean tsconfig.json",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "lint": "eslint . --ext .ts",
    "lint:commits": "commitlint",
    "build": "tsc -p .",
    "prepare": "npm run build",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p .",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "index.js",
    "bin/*.js",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "lib/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@cisco-telescope/auto-instrumentations-node": "^2.0.1",
    "@grpc/grpc-js": "^1.6.7",
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.29.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.29.2",
    "@opentelemetry/instrumentation": "^0.29.2",
    "@opentelemetry/sdk-node": "^0.29.2",
    "@opentelemetry/sdk-trace-base": "~1.3.1",
    "@opentelemetry/sdk-trace-node": "~1.3.1",
    "@opentelemetry/semantic-conventions": "~1.3.1",
    "cisco-opentelemetry-specifications": "0.0.17",
    "nyc": "^15.1.0",
    "pkginfo": "^0.4.1"
  },
  "devDependencies": {
    "@opentelemetry/resources": "^1.0.1",
    "@types/jasmine": "^3.10.3",
    "@types/jest": "^27.4.0",
    "@types/mocha": "9.0.0",
    "@types/node": "^17.0.8",
    "@types/sinon": "^10.0.6",
    "assert": "^2.0.0",
    "chai": "^4.3.6",
    "expect": "^26.6.2",
    "gts": "^3.1.0",
    "mocha": "^9.2.0",
    "nock": "^13.2.4",
    "sinon": "^12.0.1",
    "ts-mocha": "9.0.2",
    "typescript": "4.4.4"
  }
}
