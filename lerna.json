{
  "npmClient": "npm",
  "packages": [
    "packages/*"
  ],
  "version": "independent",
  "changelog": {
    "repo": "cisco-open/otel-js",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "bug": ":bug: (Bug Fix)",
      "core": ":wrench: Core",
      "document": ":books: (Refine Doc)",
      "enhancement": ":rocket: (Enhancement)",
      "feature-request": ":sparkles: (Feature)",
      "internal": ":house: Internal"
    },
    "cacheDir": ".changelog",
    "ignoreCommitters": [
      "github-actions"
    ]
  }
}
